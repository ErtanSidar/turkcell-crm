spring:
  application:
    name: "cart-service"
  data:
    mongodb:
      database: cart-service
      port: 27018
      uri: mongodb://localhost:27017

server:
  port: 8082

eureka:
  client:
    service-url:
      defaultZone: "http://localhost:8888/eureka/"
    register-with-eureka: true
    fetch-registry: true